{
  "rules": {
    "no-console": "off"
  },
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "eslint-config-prettier",
    "plugin:jest/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "impliedStrict": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "commonjs": true
  },
  "plugins": [
    "babel",
    "jest"
  ]
}
